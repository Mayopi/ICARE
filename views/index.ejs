<script>
  const scriptURL = "https://script.google.com/macros/s/AKfycbyYcKjlnscUJECaGKQ5ibiS7iWpZ1qOUDtrOdwrdBnf600G4TzMEiVWF17Nt-61npJO/exec";
  const form = document.forms["icare-contact-form"];
  const h4Response = document.querySelector(".modal-response-form");

  form.addEventListener("submit", (e) => {
    e.preventDefault();
    fetch(scriptURL, { method: "POST", body: new FormData(form) })
      .then((response) => {
        console.log("Success!", response);
      })
      .catch((error) => {
        console.log({ errors: error });
      });
  });
</script>

